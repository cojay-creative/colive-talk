{
  "name": "corailve-talk",
  "version": "1.0.0",
  "description": "Live streaming subtitle translation service",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-https": "next dev --experimental-https",
    "build": "next build",
